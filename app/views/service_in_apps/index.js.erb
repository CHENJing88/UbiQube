// render le modal d'edit techique d'app
$('#edit_tech_app_modal').html("<%= j render('service_in_apps/index')%>")
//.on("hidden.bs.modal", function() { $(this).removeData('bs.modal') })
.modal("show");
// setting pour les multiselects
triggettechmultiselect();

function triggettechmultiselect(){
  $('#vm-multiselect').multiselect({
    enableClickableOptGroups: true,
    includeSelectAllOption: true,
    enableFiltering: true,
    selectedClass: 'multiselect-selected'
  });

  $('#serv-multiselect').multiselect({
    enableClickableOptGroups: true,
    includeSelectAllOption: true,
    enableFiltering: true,
    maxHeight: 200,
    selectedClass: 'multiselect-selected'
  });
}
